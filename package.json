{
  "name": "cypress-ts",
  "version": "1.0.0",
  "description": "Cypress TypeScript Template",
  "main": "index.js",
  "scripts": {
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "format": "prettier --config .prettierrc **/*.{ts,js,json,md} --write"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/danrusu/cypress-ts.git"
  },
  "keywords": [
    "cypress",
    "typescript"
  ],
  "author": "Dan Rusu",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/danrusu/cypress-ts/issues"
  },
  "homepage": "https://github.com/danrusu/cypress-ts#readme",
  "dependencies": {
    "cypress": "^10.11.0"
  },
  "devDependencies": {
    "@badeball/cypress-cucumber-preprocessor": "^13.0.3",
    "prettier": "^2.7.1",
    "typescript": "^4.8.4"
  },
  "cypress-cucumber-preprocessor": {
    "stepDefinitions": [
      "cypress/e2e/step_definitions/**/*.ts"
    ]
